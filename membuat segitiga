#cara pertama
n = int(input("n: "))
for i in range(1, n+1):
    print(" "*n, end="")
    print("# "*i)
    n -=1

#cara kedua
n = int(input('n\t: '))
simbol = input('simbol\t: ')
for i in range(1, n+1):
  print(" "*n, end="")
  n -= 1
  print(f'{simbol} '*i)

#cara ketiga  
n = int(input('n: '))
print(*( str('# '*i).center(n*2) for i in range(1, n+1) ), sep='\n')
